#!/usr/bin/env node
var program = require('commander'),
    package = require('../package.json');

program
  .version(package.version)
  .option('-n, --number [number]', 'Number of radiodan instances [1]', 1)
  .option('-c, --config [path]', 'Location of MPD config files [mpd.conf]', './mpd.conf')
  .parse(process.argv)

console.log(program.config.split(','));

var radiodan = require('../lib/radiodan');
