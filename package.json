    "mustache": "~0.7.2",
    "tmp": "0.0.21",
    "lodash": "~2.4.1",
    "amqplib": "~0.1.1",
    "optimist": "~0.6.0",
    "q-io": "~1.10.8"
  },
  "devDependencies": {
    "mocha": "~1.17.0",
    "sinon": "~1.7.3",
    "chai": "~1.8.1",
    "chai-as-promised": "~4.1.0",
    "coveralls": "~2.7.0",
    "istanbul": "~0.2.4"
  },
  "engines": {
    "node": ">=0.10.20"
  }
