{
  "name": "radiodan",
  "preferGlobal": true,
  "version": "0.1.0",
  "author": "Dan Nuttall <dan.nuttall@bbc.co.uk>",
  "description": "Programmable radios operating at web scale",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/radiodan/radiodan.js.git"
  },
  "bin": {
    "radiodan": "./bin/radiodan"
  },
  "scripts": {
